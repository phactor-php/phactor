{
  "name": "phactor/phactor",
  "description": "PHP Message sourcing library",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "psr/log": "^1.1",
    "psr/container": "^1.0",
    "doctrine/collections": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Phactor\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhactorTest\\": "test\\unit",
      "PhactorTestMocks\\": "test\\mocks"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  }
}
